{
  "inject": {
    "kind": "rewrite",
    "regex": "(.*)",
    "variable": "$1"
  },
  "formatter": {
    "kind": "rewrite",
    "action": {
      "rewrite_regex": "&rewrite_regex+&regex",
      "target": "&target+&variable"
    },
    "rewrite_regex":"&rewrite_regex",
    "target":"&target"
  },
  "layouts": [
    {
      "key": "rewrite_regex",
      "alias": "重写前前缀",
      "type": "input",
      "help": "请输入需要替换的path前缀，必须以/开头的正则表达式配置",
      "rules": [
        "Required",
        "^/.{1,99}$"
      ]
    },
    {
      "key": "target",
      "type": "input",
      "alias": "重写后前缀",
      "help": "请输入替换的path前缀，如果想要移除前缀，则此值不填写。需要以/开头",
      "rules": [
        "^/[a-zA-Z0-9-_\\.~]+(/?[a-zA-Z0-9-_\\.~]+)*$",
        "MaxLength(100)"
      ]
    }
  ]
}